<script type="text/javascript">
	$(document).ready(function(){
		 $(".res-widget").sort(function (a, b) {
            if ( ($(a).attr("data-head").toLowerCase() > $(b).attr("data-head").toLowerCase()) )  { 
                return 1;
            } else if ( ($(a).attr("data-head").toLowerCase() == $(b).attr("data-head").toLowerCase()) ){
                return 0;
            } else {
                return -1;
            }
        }).each(function () {
            var elem = $(this);
            elem.remove();
            $(elem).appendTo("#widget_container .l-page");
        });
	});
</script>
