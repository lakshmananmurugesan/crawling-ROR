    <div id="fone-page" class="fg-12">
      <h3 class="light text-center">
          How to login to your Freshdesk account
      </h3>
<!--  <script src="http://jsmin.eu01.aws.af.cm/?project=jquerycookie" type="text/javascript"> -->
    <script src="//cdn.jsdelivr.net/jquery.cookie/1.4.0/jquery.cookie.min.js" type="text/javascript">
  </script>
  <div class="hdurl">
  </div>
  <script type="text/javascript">
    // <![CDATA[
    var _hdurl = jQuery.cookie('helpdesk_url');
    var _und;
    if(typeof _hdurl === 'undefined'){
      _und = "<p class='text-center'>Navigate to your Freshdesk account URL in your preferred browser.<br>You'll be prompted to login there with the email address that you used to sign up with Freshdesk.<br>If you forgot your URL, just enter your email below and we'll send it to you.</p></form>"
    }
    else
    {
      _und = "<p class='text-center'>The Cookie Gods have seen fit to tell us that you visited this helpdesk (<b><a href='http://" + _hdurl + "'>" + _hdurl + "</a></b>) last. If we've gotten it wrong, type in your email address and we'll take you right on over!</p></div>"
    }
    jQuery(".hdurl").html(_und);
    // ]]>
  </script>
  <div class="top-space-medium request-invite">
    <div id="status-msg">
    </div>
    <script type="text/javascript">
      // <![CDATA[
      jQuery(document).ready(function(){
        jQuery("#remind_url").validate({
          submitHandler: function(form) {
            loginVars = jQuery(form).serialize();
            jQuery.getJSON("http://signup.freshdesk.com/search_user_domain?&callback=?", loginVars, function(data){
              if (data.available){
                var succ = "Yippee! Check your inbox for an email with the list of your associated accounts. ";
                jQuery('#status-msg').addClass('grnMSG').html(succ);
              }
              else{
                var fail = "Sorry. We couldn't find any accounts associated with this email. Perhaps you signed up with another email address?" ;
                jQuery('#status-msg').toggleClass('orngMSG').html(fail);
              }
            });
      }
    });
  });
  // ]]>
  </script>

  <form id="remind_url" method="post" action="javascript:void(0)">
    
    <input type="text" name="user_email" id="user_email" class="email mktFormText mktFormEmail mktFReq text-field" maxlength="255" tabindex="1" placeholder="Enter your E-mail Address" />
    <input class="btn btn-dark remindButton" type="submit" id="remind_button" value="Remind me " />
    
  </form>
