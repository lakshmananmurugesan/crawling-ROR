<script type="text/javascript">
	$(document).ready(function(){
		$('.submit-btn').click(function() {  
			    var hasError = false;
		        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		 
		        var emailaddressVal = $(".sc-email").val();
		        if(emailaddressVal == '') {
		            $('.form-valid,.form-invalid-text').css('display','none');
		            $('.form-invalid').css('display','block');
		            hasError = true;
		        }
		 
		        else if(!emailReg.test(emailaddressVal)) {
		        	$('.form-invalid,.form-valid').css('display','none');
		            $('.form-invalid-text').css('display','block');
		            hasError = true;
		        }
		        else{
		        	$('.form-invalid,.form-invalid-text').css('display','none');
		        	$('.form-valid').css('display','block');
		        }
		        if(hasError == true) { return false; }
		 
		    });

	});


</script>
