<script type="text/javascript">
  !function( $ ) {
    
    $(function () {
      
      "use strict"
      var url = ""; 
try{
      url = getParameterByName("redirect");
}catch(e){
}
      if(!/(http|https):\/\/(.*).freshdesk.com\/(.*)/.test(url)) return;
      
      $('#account_redirect_link').attr("href",url);
      var messages = TRANSLATED.thankyoumsg;
      
      if($("#thankyou_message").get(0)){
        $.each(messages, function(i, item){
          setTimeout( function(){ 
            if(item != "_redirect")
              $("#thankyou_message").text(item);				
            else
              top.location = url;
          }, (i)*1300 );
        });		
      }
    })
    
  }(window.jQuery);
</script>
