         </span><span class="s2">&quot;time_zone&quot;</span><span class="p">:</span><span class="s2">&quot;Chennai&quot;</span><span class="p">,</span><span class="w">
         </span><span class="s2">&quot;twitter_id&quot;</span><span class="p">:</span><span class="s2">&quot;&quot;</span><span class="p">,</span><span class="w">
         </span><span class="s2">&quot;updated_at&quot;</span><span class="p">:</span><span class="s2">&quot;2013-12-20T15:04:16+05:30&quot;</span><span class="p">,</span><span class="w">
         </span><span class="s2">&quot;custom_field&quot;</span><span class="w"> </span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
            </span><span class="s2">&quot;cf_department&quot;</span><span class="w"> </span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;IT&quot;</span><span class="p">,</span><span class="w">
            </span><span class="s2">&quot;cf_date_of_joining&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;2014-12-01T00:00:00Z&quot;</span><span class="w">
         </span><span class="p">}</span><span class="w">
      </span><span class="p">}</span><span class="w">
   </span><span class="p">},</span><span class="w">
   </span><span class="err">...</span><span class="w">
</span><span class="p">]</span></td></tr></tbody></table></pre>
