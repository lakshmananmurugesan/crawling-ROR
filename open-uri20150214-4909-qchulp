<span class="n">site</span> <span class="o">=</span> <span class="no">RestClient</span><span class="o">::</span><span class="no">Resource</span><span class="nf">.new</span><span class="p">(</span><span class="s2">&quot;http://domain.freshdesk.com/helpdesk/tickets/</span><span class="si">#{</span><span class="n">value</span><span class="si">}</span><span class="s2">.json&quot;</span><span class="p">,</span><span class="s2">&quot;user@yourcompany.com&quot;</span><span class="p">,</span><span class="s2">&quot;test&quot;</span><span class="p">)</span>
