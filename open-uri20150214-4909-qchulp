               </span><span class="s2">&quot;address&quot;</span><span class="p">:</span><span class="err">null</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;created_at&quot;</span><span class="p">:</span><span class="s2">&quot;2013-12-11T18:11:27+05:30&quot;</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;customer_id&quot;</span><span class="p">:</span><span class="err">null</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;deleted&quot;</span><span class="p">:</span><span class="kc">false</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;description&quot;</span><span class="p">:</span><span class="err">null</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;email&quot;</span><span class="p">:</span><span class="s2">&quot;popstart@yourcompany.com&quot;</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;external_id&quot;</span><span class="p">:</span><span class="err">null</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;fb_profile_id&quot;</span><span class="p">:</span><span class="err">null</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;helpdesk_agent&quot;</span><span class="p">:</span><span class="kc">true</span><span class="p">,</span><span class="w">
               </span><span class="s2">&quot;id&quot;</span><span class="p">:</span><span class="mi">16</span><span class="p">,</span><span class="w">
