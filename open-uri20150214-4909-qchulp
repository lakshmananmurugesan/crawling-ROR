<span class="c1">#contacts can also be filtered by phone,email etc</span>

<span class="n">params</span> <span class="o">=</span> <span class="no">URI</span><span class="nf">.escape</span><span class="p">(</span><span class="s2">&quot;query=phone is 123245890&quot;</span><span class="p">)</span>

