<span class="c1"># Need to specify category_id,forum_id,topic_id in url</span>
<span class="c1"># eg:</span>
<span class="c1"># site = RestClient::Resource.new(&quot;http://domain.freshdesk.com/categories/2/forums/5/topics/1/monitorship.json&quot;,&quot;user@yourcompany.com&quot;,&quot;test&quot;)</span>

<span class="n">site</span> <span class="o">=</span> <span class="no">RestClient</span><span class="o">::</span><span class="no">Resource</span><span class="nf">.new</span><span class="p">(</span><span class="s2">&quot;http://domain.freshdesk.com/categories/[category_id]/forums/[forum_id]/topics/[topic_id]/monitorship.json&quot;</span><span class="p">,</span><span class="s2">&quot;user@yourcompany.com&quot;</span><span class="p">,</span><span class="s2">&quot;test&quot;</span><span class="p">)</span>

<span class="n">response</span><span class="o">=</span><span class="n">site</span><span class="nf">.post</span><span class="p">(</span><span class="ss">:content_type</span><span class="o">=&gt;</span><span class="s2">&quot;application/json&quot;</span><span class="p">)</span>

<span class="nb">puts</span> <span class="s2">&quot;response: </span><span class="si">#{</span><span class="n">response</span><span class="nf">.code</span><span class="si">}</span><span class="s2"> </span><span class="se">\n</span><span class="s2"> </span><span class="si">#{</span><span class="n">response</span><span class="nf">.body</span><span class="si">}</span><span class="s2">&quot;</span></td></tr></tbody></table></pre>
